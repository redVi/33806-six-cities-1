{
  "name": "six-cities",
  "version": "1.0.0",
  "private": true,
  "description": "Личный проект «Шесть городов» от HTML Academy",
  "scripts": {
    "build": "webpack --mode development",
    "start": "webpack-dev-server",
    "eslint": "eslint src/**/*.js",
    "test": "npm run eslint && jest"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "leaflet": "1.5.1",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-redux": "7.0.3",
    "react-router-dom": "5.0.0",
    "redux": "4.0.1",
    "redux-thunk": "2.3.0",
    "reselect": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.4.4",
    "@babel/preset-env": "7.4.4",
    "@babel/preset-react": "7.0.0",
    "@types/enzyme": "3.9.3",
    "@types/enzyme-adapter-react-16": "1.0.5",
    "@types/jest": "24.0.13",
    "@types/react": "16.8.19",
    "@types/react-redux": "^7.0.9",
    "@types/react-router-dom": "^4.3.3",
    "babel-jest": "24.7.1",
    "babel-loader": "8.0.5",
    "enzyme": "3.9.0",
    "enzyme-adapter-react-16": "1.12.1",
    "eslint": "5.16.0",
    "eslint-config-htmlacademy": "0.5.2",
    "eslint-plugin-react": "7.12.4",
    "jest": "^24.8.0",
    "react-test-renderer": "16.8.6",
    "ts-jest": "^24.0.2",
    "ts-loader": "6.0.2",
    "typescript": "3.5.1",
    "utility-types": "3.7.0",
    "webpack": "4.30.0",
    "webpack-cli": "3.3.1",
    "webpack-dev-server": "3.3.1"
  },
  "engines": {
    "node": "10"
  },
  "jest": {
    "moduleNameMapper": {
      "@/(.*)$": "<rootDir>/src/$1"
    },
    "setupFiles": [
      "./src/setup-tests.ts"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest",
      "^.+\\.jsx?$": "babel-jest"
    },
    "testRegex": ".test.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
